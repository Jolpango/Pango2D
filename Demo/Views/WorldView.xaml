<StackPanel
	Id="Panel"
	Gap="10"
	Orientation="Vertical"
	Padding="10, 10"
	Anchor="TopLeft">
	<P
		Text="{Bind:FPSText}"
		FontColor="rgb(255, 255, 255)"
		MinSize="150, 50"
		TextAlign="Center"
		BackgroundColor="rgba(0, 0, 0, 0)"
		BackgroundImage="UI/BluePanel"
		NineSlice="8:8:8:8:4,0"
		></P>
</StackPanel>