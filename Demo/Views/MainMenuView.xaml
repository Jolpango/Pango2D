<StackPanel
	Id="Panel"
	Gap="10"
	Orientation="Vertical"
	Padding="10, 10"
	BackgroundImage="{Bind:BackgroundImage}"
	ImageOptions="Center"
	Anchor="TopRight"
	BackgroundColor="rgb(255, 0, 0)">
	<Label Text="Hello from property" OnClick="OnClick"/>
	<Label OnClick="OnClick">Hello from inside</Label>
	<Label OnClick="OnClick" Text="Hello from priority property">Should not see me</Label>
	<H1 FontSize="72" FontStyle="Bold" FontColor="rgb(255, 255, 255)">Header1</H1>
	<H2 FontColor="rgb(255, 255, 255)">Header2</H2>
	<H3 FontColor="rgb(255, 255, 255)">Header3</H3>
	<Button OnClick="OnClick">START!</Button>
</StackPanel>